        <span class="kw4">bool</span> atomic_compare_exchange_strong_explicit<span class="br0">&#40;</span>
            shared_ptr<span class="sy1">&lt;</span>T<span class="sy1">&gt;</span><span class="sy2">*</span> p, shared_ptr<span class="sy1">&lt;</span>T<span class="sy1">&gt;</span><span class="sy2">*</span> v, shared_ptr<span class="sy1">&lt;</span>T<span class="sy1">&gt;</span> w,
            memory_order success, memory_order failure<span class="br0">&#41;</span><span class="sy4">;</span>
&#160;
    <span class="co1">//  hash support</span>
    <span class="kw1">template</span> <span class="sy1">&lt;</span><span class="kw1">class</span> T<span class="sy1">&gt;</span> <span class="kw1">struct</span> hash<span class="sy4">;</span>
    <span class="kw1">template</span> <span class="sy1">&lt;</span><span class="kw1">class</span> T, <span class="kw1">class</span> D<span class="sy1">&gt;</span> <span class="kw1">struct</span> hash<span class="sy1">&lt;</span>unique_ptr<span class="sy1">&lt;</span>T, D<span class="sy1">&gt;</span> <span class="sy1">&gt;</span><span class="sy4">;</span>
    <span class="kw1">template</span> <span class="sy1">&lt;</span><span class="kw1">class</span> T<span class="sy1">&gt;</span> <span class="kw1">struct</span> hash<span class="sy1">&lt;</span>shared_ptr<span class="sy1">&lt;</span>T<span class="sy1">&gt;</span> <span class="sy1">&gt;</span><span class="sy4">;</span>
&#160;
    <span class="co1">// auto_ptr (deprecated)</span>
    <span class="kw1">template</span> <span class="sy1">&lt;</span><span class="kw1">class</span> X<span class="sy1">&gt;</span> <span class="kw1">class</span> auto_ptr<span class="sy4">;</span>
<span class="br0">&#125;</span></pre></div></div>
<table class="metadata plainlinks ambox mbox-small-left ambox-notice" style=""><tr><td class="mbox-empty-cell"></td><td class="mbox-text" style="">This section is incomplete<br />Reason: Class synopses </td></tr></table>

